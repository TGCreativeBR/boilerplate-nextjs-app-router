    "cookie": "^0.5.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "negotiator": "^0.6.3",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.1.8",
    "typescript": "5.4.2",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.4.0",
